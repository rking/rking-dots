loadplugins '\.(js|penta)$'
group user
map -modes=n,v <C-F5> -ex :rel|viewsource
map -count -modes=n,v <F2> <count>:dia<Space>pageinfo<Return>
map -count -modes=n,v <F3> <count>:styletoggle<Space>-group<Space>user<Return>
map -modes=n,v h -ex back
map -modes=n,v l -ex forward
set defsearch=duck-duck-go-google-suggest
set editor='~/bin/penta-vim -f +<line> +"sil! call cursor(0, <column>)" <file>'
set findflags=r
set guioptions=bCrsnc
set hintkeys=qwertyuipasdfghjklzxcvnm
set passkeys=duckduckgo.com:jk<cr>,mail.google.com:jkroae<cr>
set strictfocus='chrome:*':laissez-faire,*:despotic

