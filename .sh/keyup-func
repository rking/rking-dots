keyup() {
    if type keychain >& /dev/null; then
        for n in ~/.ssh/id{entity,_{d,ecd,r}sa}; do
            [ -e $n ] && keychain -q $n
        done
        echo -e '\e[36mkeychain\e[0med.'
    else
        echo "keychain not found (can be installed as a distro pkg or in ~/bin)"
    fi
}
# vim:ft=sh
